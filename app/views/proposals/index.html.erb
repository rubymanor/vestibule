<h1>Talk proposals</h1>

<ul class="proposals">
  <% for proposal in @proposals %>
    <li><%= link_to proposal_title_with_modification_time(proposal), proposal %></li>
  <% end %>
</ul>

<% if user_signed_in? %>
  <%= link_to "Propose talk", new_proposal_path %>
<% end %>

<% content_for :head do %>
  <%= auto_discovery_link_tag :rss, :format => :rss %>
<% end %>
